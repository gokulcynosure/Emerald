<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>QLI_Total_Price_Temp__c</fullName>
    <description>Here we calculate total amount after discounts</description>
    <externalId>false</externalId>
    <formula>(UnitPrice - (ROUND(UnitPrice * Quote_Discount__c, 2))) * (1 - ( PPD__c +  VD__c +  SPL_DISCOUNT__c ))</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Total Price Temp</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <type>Currency</type>
</CustomField>
