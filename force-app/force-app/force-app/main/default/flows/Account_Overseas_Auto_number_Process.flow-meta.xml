    <formulas>
        <name>LeadzoneFormat</name>
        <dataType>String</dataType>
        <expression>&quot;EM-O-&quot; + {!ZoneCode} +&quot;-&quot;+ LPAD(TEXT({!ZonePreviousNo} + 1), 6, &apos;0&apos;)</expression>
    </formulas>
    <formulas>
