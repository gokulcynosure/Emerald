    </formulas>
    <formulas>
        <name>ZoneNo</name>
        <dataType>String</dataType>
        <expression>RIGHT({!Get_Previous_Lead_Records.OverseasNo__c}, LEN({!Get_Previous_Lead_Records.OverseasNo__c}) - FIND(&quot;-&quot;, {!Get_Previous_Lead_Records.OverseasNo__c}) - 1)</expression>
    </formulas>
    <formulas>
